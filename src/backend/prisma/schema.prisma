// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  email String @unique
  name String 
  password String? //VARCHAR   //Método tradicional
  googleId String? //Método de login con Google
  avatar String? //Obtener foto de perfil
}